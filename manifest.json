{
  "name": "Google Quick Search",
  "description": "This app quickly opens first n tabs from google search engine results",
  "manifest_version": 2,
  "minimum_chrome_version": "28",
  "offline_enabled": true,
  "version": "0.2",
  "permissions": [
    "tabs",
    "clipboardWrite"
  ],
  "background": {
    "scripts": [
      "js/bg.js"
    ]
  },
  "content_scripts":
  [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "js/vendor/jquery.js.min",
//        "js/vendor/underscore.js.min",
        "js/content.js"
      ]
    }
  ],
  "icons": {
//    "16": "images/icon-16x16.png",
//    "128": "images/icon-128x128.png"
  },
  "commands": {
    "search" : {
      "suggested_key": {
        "default": "Ctrl+Shift+K"
      },
      "description": "Open first links"
    }
  },
  "platforms": [
//    {
//      "nacl_arch": "x86-64",
//      "sub_package_path": "_platform_specific/x86-64/"
//    },
//    {
//      "nacl_arch": "x86-32",
//      "sub_package_path": "_platform_specific/x86-32/"
//    },
//    {
//      "nacl_arch": "arm",
//      "sub_package_path": "_platform_specific/arm/"
//    },
//    {
//      "sub_package_path": "_platform_specific/all/"
//    }
  ]
}